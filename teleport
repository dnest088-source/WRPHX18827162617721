-- Warpah Teleport v3.0 - Professional GUI with Folder System
-- Ultra-compact design with folders organization + Play All + Auto Loop

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Saved teleports storage with folder system
local savedTeleports = {}
local folders = {}
local currentFolder = nil -- nil = root/no folder
local isPlayingAll = false
local playAllDelay = 3
local autoRespawn = true
local autoLoop = false

-- Data persistence
local SAVE_FILE_NAME = "WarpahTeleportData_" .. game.PlaceId .. ".json"

local function saveDataToFile()
    local success, err = pcall(function()
        local data = {
            teleports = savedTeleports,
            folders = folders,
            settings = {
                autoRespawn = autoRespawn,
                autoLoop = autoLoop,
                delay = playAllDelay
            }
        }
        local jsonData = HttpService:JSONEncode(data)
        writefile(SAVE_FILE_NAME, jsonData)
    end)
    if not success then
        warn("Failed to save data: " .. tostring(err))
    end
end

local function loadDataFromFile()
    local success, result = pcall(function()
        if isfile(SAVE_FILE_NAME) then
            local jsonData = readfile(SAVE_FILE_NAME)
            local data = HttpService:JSONDecode(jsonData)
            return data
        end
        return nil
    end)
    
    if success and result then
        return result
    else
        return nil
    end
end

