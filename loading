-- Enhanced Loading Messages
local loadingMessages = {
    "Initializing...",
    "Bypassing security...",
    "Loading modules...",
    "Bypassing anticheat...",
    "Connecting servers...",
    "Bypassing detection...",
    "Finalizing injection...",
    "Ready!"
}

-- Synchronized Loading System
local progress = 0
local messageIndex = 1

local function updateProgress()
    progress = progress + math.random(3, 6)
    if progress > 100 then progress = 100 end
    
    -- Smooth synchronized progress bar
    local progressTween = TweenService:Create(loadingFill,
        TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
        {Size = UDim2.new(progress / 100, 0, 1, 0)}
    )
    progressTween:Play()
    
    -- Update synchronized status
    progressLabel.Text = progress .. "%"
    
    if messageIndex <= #loadingMessages then
        statusText.Text = loadingMessages[messageIndex]
        if progress >= (messageIndex * 100 / #loadingMessages) then
            messageIndex = messageIndex + 1
        end
    end
    
    -- Complete loading
    if progress >= 100 then
        wait(1.2)
        statusText.Text = "WarpahVip Ready!"
        
        -- Synchronized exit animations
        local exitTween = TweenService:Create(outerContainer,
            TweenInfo.new(1.2, Enum.EasingStyle.Back, Enum.EasingDirection.In),
            {Position = UDim2.new(0.5, -235, -1.5, 0), Size = UDim2.new(0, 0, 0, 0)}
        )
        
        exitTween:Play()
        
        exitTween.Completed:Connect(function()
            screenGui:Destroy()
            print("⚡ WarpahVip Exploit loaded successfully!")
            -- Your main exploit code here
        end)
        
        return
    end
end

-- Run Synchronized Loading
spawn(function()
    wait(2.8) -- Wait for entrance animations to complete
    while progress < 100 do
        updateProgress()
        wait(math.random(120, 280) / 1000)
    end
end)
