-- WarpahVip Professional Loading Screen
-- Enhanced Particles Version - More Beautiful & Eye-Comfortable
-- Compatible with Delta Executor Android

local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

-- Create main GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "WarpahVipPro"
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = game.CoreGui

-- Transparent Main Frame (No background - game visible)
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(1, 0, 1, 0)
mainFrame.Position = UDim2.new(0, 0, 0, 0)
mainFrame.BackgroundTransparency = 1
mainFrame.BorderSizePixel = 0
mainFrame.ZIndex = 1
mainFrame.Parent = screenGui

-- Professional Outer Container Frame (Neon Border Structure)
local outerContainer = Instance.new("Frame")
outerContainer.Name = "OuterContainer"
outerContainer.Size = UDim2.new(0, 470, 0, 240)
outerContainer.Position = UDim2.new(0.5, -235, 0.5, -120)
outerContainer.BackgroundTransparency = 1
outerContainer.BorderSizePixel = 0
outerContainer.ZIndex = 4
outerContainer.ClipsDescendants = true -- Important: Clips content to container
outerContainer.Parent = mainFrame

-- Structured Neon Border (Outside container)
local neonBorder = Instance.new("UIStroke")
neonBorder.Color = Color3.fromRGB(0, 255, 150)
neonBorder.Thickness = 3
neonBorder.Transparency = 0.3
neonBorder.Parent = outerContainer

local outerCorner = Instance.new("UICorner")
outerCorner.CornerRadius = UDim.new(0, 25)
outerCorner.Parent = outerContainer

-- BLACK BACKGROUND - Only Inside Container
local blackBackground = Instance.new("Frame")
blackBackground.Name = "BlackBackground"
blackBackground.Size = UDim2.new(1, -6, 1, -6)
blackBackground.Position = UDim2.new(0, 3, 0, 3)
blackBackground.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
blackBackground.BackgroundTransparency = 0.1 -- Slightly more opaque
blackBackground.BorderSizePixel = 0
blackBackground.ZIndex = 5
blackBackground.Parent = outerContainer

local blackCorner = Instance.new("UICorner")
blackCorner.CornerRadius = UDim.new(0, 22)
blackCorner.Parent = blackBackground

-- ENHANCED STARFIELD - More Beautiful & Eye-Comfortable
local starField = Instance.new("Frame")
starField.Size = UDim2.new(1, 0, 1, 0)
starField.BackgroundTransparency = 1
starField.ZIndex = 6
starField.Parent = blackBackground

-- Create Enhanced Stars ONLY Inside Container
for i = 1, 35 do -- Increased count for richer background
    local star = Instance.new("Frame")
    local size = math.random(1, 3) -- Varied sizes
    star.Size = UDim2.new(0, size, 0, size)
    star.Position = UDim2.new(math.random(3, 97) / 100, 0, math.random(3, 97) / 100, 0)
    
    -- Softer, more comfortable star colors
    local starColors = {
        Color3.fromRGB(200, 230, 255), -- Soft blue-white
        Color3.fromRGB(255, 240, 200), -- Warm white
        Color3.fromRGB(220, 255, 220), -- Soft green-white
        Color3.fromRGB(255, 220, 240), -- Soft pink-white
        Color3.fromRGB(240, 255, 200)  -- Soft yellow-white
    }
    star.BackgroundColor3 = starColors[math.random(1, #starColors)]
    star.BorderSizePixel = 0
    star.ZIndex = 7
    star.Parent = starField
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = star
    
    -- Enhanced twinkling with varying speeds
    local twinkleSpeed = math.random(2, 8)
    local minTransparency = math.random(3, 6) / 10
    local maxTransparency = math.random(7, 9) / 10
    
    local twinkle = TweenService:Create(star, 
        TweenInfo.new(twinkleSpeed, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {BackgroundTransparency = maxTransparency}
    )
    twinkle:Play()
    
    -- Initial transparency for softer appearance
    star.BackgroundTransparency = minTransparency
end

-- ENHANCED PARTICLE SYSTEM - Multiple Layers & Effects
local particleField = Instance.new("Frame")
particleField.Size = UDim2.new(1, 0, 1, 0)
particleField.BackgroundTransparency = 1
particleField.ZIndex = 6
particleField.Parent = blackBackground

-- Layer 1: Main Green Particles (Larger, Slower)
for i = 1, 6 do
    local particle = Instance.new("Frame")
    particle.Size = UDim2.new(0, math.random(3, 5), 0, math.random(3, 5))
    particle.Position = UDim2.new(math.random(10, 90) / 100, 0, math.random(10, 90) / 100, 0)
    particle.BackgroundColor3 = Color3.fromRGB(0, 255, 150)
    particle.BorderSizePixel = 0
    particle.ZIndex = 7
    particle.Parent = particleField
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = particle
    
    -- Smooth orbital-like movement
    local centerX = math.random(20, 80) / 100
    local centerY = math.random(20, 80) / 100
    local radius = math.random(10, 25) / 100
    local speed = math.random(15, 25)
    
    spawn(function()
        local angle = math.random(0, 360)
        while particle.Parent do
            angle = angle + 1
            local x = centerX + math.cos(math.rad(angle)) * radius
            local y = centerY + math.sin(math.rad(angle)) * radius
            
            -- Ensure particles stay within bounds
            x = math.clamp(x, 0.05, 0.95)
            y = math.clamp(y, 0.05, 0.95)
            
            particle.Position = UDim2.new(x, 0, y, 0)
            wait(0.03)
        end
    end)
    
    -- Gentle pulsing glow
    local glow = TweenService:Create(particle,
        TweenInfo.new(math.random(3, 6), Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {BackgroundTransparency = math.random(4, 7) / 10}
    )
    glow:Play()
end

-- Layer 2: Small Cyan Accent Particles
for i = 1, 8 do
    local particle = Instance.new("Frame")
    particle.Size = UDim2.new(0, math.random(1, 2), 0, math.random(1, 2))
    particle.Position = UDim2.new(math.random(15, 85) / 100, 0, math.random(15, 85) / 100, 0)
    particle.BackgroundColor3 = Color3.fromRGB(0, 200, 255)
    particle.BorderSizePixel = 0
    particle.ZIndex = 7
    particle.Parent = particleField
    particle.BackgroundTransparency = 0.3
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = particle
    
    -- Fast linear movement with wrapping
    spawn(function()
        local speedX = (math.random(-2, 2)) / 100
        local speedY = (math.random(-2, 2)) / 100
        while particle.Parent do
            local currentX = particle.Position.X.Scale + speedX
            local currentY = particle.Position.Y.Scale + speedY
            
            -- Wrap around edges
            if currentX > 0.95 then currentX = 0.05 end
            if currentX < 0.05 then currentX = 0.95 end
            if currentY > 0.95 then currentY = 0.05 end
            if currentY < 0.05 then currentY = 0.95 end
            
            particle.Position = UDim2.new(currentX, 0, currentY, 0)
            wait(0.05)
        end
    end)
    
    -- Rapid flickering
    local flicker = TweenService:Create(particle,
        TweenInfo.new(math.random(1, 2), Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true),
        {BackgroundTransparency = math.random(6, 9) / 10}
    )
    flicker:Play()
end

-- Layer 3: Floating Light Orbs (Soft & Comfortable)
for i = 1, 4 do
    local orb = Instance.new("Frame")
    orb.Size = UDim2.new(0, math.random(6, 10), 0, math.random(6, 10))
    orb.Position = UDim2.new(math.random(20, 80) / 100, 0, math.random(20, 80) / 100, 0)
    
    -- Soft gradient colors
    local orbColors = {
        Color3.fromRGB(100, 255, 200),
        Color3.fromRGB(150, 255, 150),
        Color3.fromRGB(50, 255, 180)
    }
    orb.BackgroundColor3 = orbColors[math.random(1, #orbColors)]
    orb.BorderSizePixel = 0
    orb.ZIndex = 6
    orb.BackgroundTransparency = 0.6
    orb.Parent = particleField
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = orb
    
    -- Add soft gradient
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(1, orb.BackgroundColor3)
    }
    gradient.Parent = orb
    
    -- Very slow, dreamy movement
    local dreamFloat = TweenService:Create(orb, 
        TweenInfo.new(math.random(20, 35), Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {Position = UDim2.new(math.random(20, 80) / 100, 0, math.random(20, 80) / 100, 0)}
    )
    dreamFloat:Play()
    
    -- Gentle breathing effect
    local breathe = TweenService:Create(orb,
        TweenInfo.new(math.random(8, 12), Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {Size = UDim2.new(0, orb.Size.X.Offset + 3, 0, orb.Size.Y.Offset + 3), BackgroundTransparency = 0.8}
    )
    breathe:Play()
end

-- Layer 4: Sparkling Micro Particles
for i = 1, 15 do
    local spark = Instance.new("Frame")
    spark.Size = UDim2.new(0, 1, 0, 1)
    spark.Position = UDim2.new(math.random(10, 90) / 100, 0, math.random(10, 90) / 100, 0)
    spark.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    spark.BorderSizePixel = 0
    spark.ZIndex = 8
    spark.BackgroundTransparency = 0.2
    spark.Parent = particleField
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = spark
    
    -- Random sparkle effect
    spawn(function()
        while spark.Parent do
            -- Random appear/disappear
            if math.random(1, 100) <= 5 then -- 5% chance each frame
                local sparkle = TweenService:Create(spark,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                    {Size = UDim2.new(0, 3, 0, 3), BackgroundTransparency = 0}
                )
                sparkle:Play()
                
                sparkle.Completed:Connect(function()
                    local fade = TweenService:Create(spark,
                        TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In),
                        {Size = UDim2.new(0, 1, 0, 1), BackgroundTransparency = 0.8}
                    )
                    fade:Play()
                end)
            end
            wait(0.1)
        end
    end)
end

