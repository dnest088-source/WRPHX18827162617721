local Camera = Workspace.CurrentCamera

-- FPS Booster Configuration
local FPSBooster = {
    Enabled = false,
    RemovedObjects = {},
    OriginalObjects = {},
    Settings = {
        RemoveTrees = true,
        RemoveGrass = true,
        RemoveRocks = true,
        RemoveParticles = true,
        RemoveDecals = true,
        RemoveTextures = true,
        ReduceLighting = true,
        DisableShadows = true,
        LowQualityMode = true
    }
}

