FPSLabel.Name = "FPSLabel"
FPSLabel.Size = UDim2.new(1, -8, 1, 0)
FPSLabel.Position = UDim2.new(0, 4, 0, 0)
FPSLabel.BackgroundTransparency = 1
FPSLabel.Text = "FPS: 60"
FPSLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
