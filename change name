StatusLabel.TextXAlignment = Enum.TextXAlignment.Center

-- Fungsi untuk update color preview
local function UpdateColorPreview()
    local r = math.clamp(tonumber(RedSlider.Text) or 255, 0, 255)
    local g = math.clamp(tonumber(GreenSlider.Text) or 255, 0, 255)
    local b = math.clamp(tonumber(BlueSlider.Text) or 255, 0, 255)
    
    ColorPreview.BackgroundColor3 = Color3.fromRGB(r, g, b)
    PreviewLabel.TextColor3 = Color3.fromRGB(r, g, b)
end

-- Update preview saat mengetik
RedSlider:GetPropertyChangedSignal("Text"):Connect(UpdateColorPreview)
GreenSlider:GetPropertyChangedSignal("Text"):Connect(UpdateColorPreview)
BlueSlider:GetPropertyChangedSignal("Text"):Connect(UpdateColorPreview)

-- Fungsi untuk mengubah nickname
local function ChangeNickname()
    local newName = NicknameInput.Text
    local textSize = tonumber(SizeInput.Text) or 14
    local r = math.clamp(tonumber(RedSlider.Text) or 255, 0, 255)
    local g = math.clamp(tonumber(GreenSlider.Text) or 255, 0, 255)
    local b = math.clamp(tonumber(BlueSlider.Text) or 255, 0, 255)
    
    if newName == "" then
        StatusLabel.Text = "Status: Please enter a nickname!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    -- Cari character player
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local head = character:WaitForChild("Head")
    
    -- Hapus nameplate lama jika ada
    for _, child in pairs(head:GetChildren()) do
        if child:IsA("BillboardGui") then
            child:Destroy()
        end
    end
    
    -- Buat nameplate baru
    local BillboardGui = Instance.new("BillboardGui")
    BillboardGui.Name = "CustomNameplate"
    BillboardGui.Parent = head
    BillboardGui.Size = UDim2.new(0, 200, 0, 50)
    BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
    BillboardGui.AlwaysOnTop = true
    
    local TextLabel = Instance.new("TextLabel")
    TextLabel.Parent = BillboardGui
    TextLabel.BackgroundTransparency = 1
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Font = Enum.Font.GothamBold
    TextLabel.Text = newName
    TextLabel.TextColor3 = Color3.fromRGB(r, g, b)
    TextLabel.TextSize = textSize
    TextLabel.TextStrokeTransparency = 0.5
    TextLabel.TextScaled = false
    
    StatusLabel.Text = "Status: Nickname changed successfully!"
    StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
end

-- Fungsi untuk minimize
local isMinimized = false
MinimizeBtn.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    
    if isMinimized then
        ContentFrame.Visible = false
        MainFrame:TweenSize(UDim2.new(0, 280, 0, 35), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.3, true)
    else
        MainFrame:TweenSize(UDim2.new(0, 280, 0, 330), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.3, true)
        wait(0.3)
        ContentFrame.Visible = true
    end
end)

-- Fungsi untuk close
CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- Fungsi untuk apply button
ApplyBtn.MouseButton1Click:Connect(function()
    ChangeNickname()
end)

-- Auto-update saat character respawn
LocalPlayer.CharacterAdded:Connect(function(character)
    if NicknameInput.Text ~= "" then
        wait(1)
        ChangeNickname()
    end
end)

print("Nickname Changer Script Loaded!")
print("Made for Delta Executor")
