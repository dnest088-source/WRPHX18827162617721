-- Fungsi untuk scan unanchored parts
local function scanUnanchoredParts()
    unanchoredParts = {}
    local count = 0
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") and not obj.Anchored and obj.Name ~= "Baseplate" then
            local isPlayerPart = false
            local parent = obj.Parent
            
            while parent do
                if parent:IsA("Model") then
                    if Players:GetPlayerFromCharacter(parent) then
                        isPlayerPart = true
                        break
                    end
                    if parent:FindFirstChildOfClass("Humanoid") then
                        isPlayerPart = true
                        break
                    end
                end
                parent = parent.Parent
            end
            
            if not isPlayerPart then
                table.insert(unanchoredParts, obj)
                count = count + 1
            end
        end
    end
    
    return count
end

-- Fungsi untuk delete SEMUA constraint dan rope - OPTIMIZED
local function deleteAllConstraints(part)
    if processedParts[part] and tick() - processedParts[part] < 1 then
        return
    end
    
    processedParts[part] = tick()
    
    pcall(function()
        local constraintsToDelete = {}
        
        for _, child in pairs(part:GetChildren()) do
            if child:IsA("Constraint") then
                table.insert(constraintsToDelete, child)
            end
        end
        
        for _, constraint in pairs(constraintsToDelete) do
            constraint:Destroy()
        end
        
        for _, attachment in pairs(part:GetChildren()) do
            if attachment:IsA("Attachment") then
                for _, constraint in pairs(attachment:GetChildren()) do
                    if constraint:IsA("Constraint") then
                        constraint:Destroy()
                    end
                end
            end
        end
        
        local partPosition = part.Position
        local searchRadius = 50
        
        for _, obj in pairs(Workspace:GetDescendants()) do
            if obj:IsA("Constraint") then
                local isNearby = false
                
                if obj:IsA("RopeConstraint") or obj:IsA("RodConstraint") or 
                   obj:IsA("SpringConstraint") then
                    if obj.Attachment0 and obj.Attachment0.Parent then
                        local dist = (obj.Attachment0.WorldPosition - partPosition).Magnitude
                        if dist < searchRadius then
                            isNearby = true
                        end
                    end
                end
                
                if isNearby then
                    pcall(function()
                        if obj.Attachment0 and obj.Attachment1 then
                            if obj.Attachment0.Parent == part or obj.Attachment1.Parent == part then
                                obj:Destroy()
                            end
                        elseif obj:IsA("WeldConstraint") or obj:IsA("RigidConstraint") then
                            if obj.Part0 == part or obj.Part1 == part then
                                obj:Destroy()
                            end
                        end
                    end)
                end
            end
        end
    end)
end

-- Fungsi bring dengan physical manipulation - OPTIMIZED
local currentPartIndex = 1

local function bringPartsPhysical()
    if not targetPlayer or not targetPlayer.Character then
        return
    end
    
    local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not targetRoot then
        return
    end
    
    if tick() - lastScanTime > scanCooldown then
        scanUnanchoredParts()
        lastScanTime = tick()
    end
    
    local partsProcessed = 0
    local startIndex = currentPartIndex
    
    for i = startIndex, #unanchoredParts do
        if partsProcessed >= maxPartsPerFrame then
            currentPartIndex = i + 1
            if currentPartIndex > #unanchoredParts then
                currentPartIndex = 1
            end
            return
        end
        
        local part = unanchoredParts[i]
        
        if part and part.Parent and part:IsA("BasePart") then
            local success = pcall(function()
                local distance = (part.Position - targetRoot.Position).Magnitude
                if distance > 500 then
                    return
                end
                
                local hasConstraints = false
                for _, child in pairs(part:GetChildren()) do
                    if child:IsA("Constraint") then
                        hasConstraints = true
                        break
                    end
                end
                
                if hasConstraints then
                    deleteAllConstraints(part)
                end
                
                if part:CanSetNetworkOwnership() then
                    part:SetNetworkOwner(LocalPlayer)
                end
                
                local bodyPos = part:FindFirstChildOfClass("BodyPosition")
                local bodyGyro = part:FindFirstChildOfClass("BodyGyro")
                
                if not bodyPos then
                    bodyPos = Instance.new("BodyPosition")
                    bodyPos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                    bodyPos.D = Settings.Damping
                    bodyPos.P = Settings.Power
                    bodyPos.Parent = part
                    table.insert(bodyMovers, {part = part, mover = bodyPos})
                end
                
                if not bodyGyro and distance < 100 then
                    bodyGyro = Instance.new("BodyGyro")
                    bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
                    bodyGyro.D = 2000
                    bodyGyro.P = 20000
                    bodyGyro.CFrame = part.CFrame
                    bodyGyro.Parent = part
                    table.insert(bodyMovers, {part = part, mover = bodyGyro})
                end
                
                -- FIXED: Parts berputar di sekitar target, TIDAK menabrak langsung
                local angle = tick() * 2 + (i * 0.5) -- Rotasi berbeda untuk tiap part
                local radius = math.clamp(distance * 0.3, 8, 15) -- Radius orbit 8-15 studs
                
                local offset = Vector3.new(
                    math.cos(angle) * radius,
                    math.sin(tick() * 3 + i) * 5 + 8, -- Naik turun smooth
                    math.sin(angle) * radius
                )
                
                if bodyPos then
                    -- Target posisi adalah SEKITAR player, bukan tepat di player
                    bodyPos.Position = targetRoot.Position + offset
                end
                
                part.CanCollide = false
                
                -- Velocity boost hanya untuk part yang jauh
                if distance > 30 then
                    local direction = (targetRoot.Position + offset - part.Position).Unit
                    part.AssemblyLinearVelocity = direction * Settings.VelocityBoost
                else
                    -- Kurangi kecepatan saat sudah dekat untuk tidak menabrak
                    part.AssemblyLinearVelocity = part.AssemblyLinearVelocity * 0.8
                end
            end)
            
            if success then
                partsProcessed = partsProcessed + 1
            end
        end
    end
    
    if currentPartIndex > #unanchoredParts then
        currentPartIndex = 1
    end
end

-- Fungsi cleanup body movers - OPTIMIZED
local function cleanupBodyMovers()
    local moversToClean = {}
    
    for _, data in pairs(bodyMovers) do
        if data.part and data.part.Parent and data.mover then
            table.insert(moversToClean, data)
        end
    end
    
    for i, data in ipairs(moversToClean) do
        pcall(function()
            data.mover:Destroy()
            if data.part then
                data.part.CanCollide = true
            end
        end)
        
        if i % 10 == 0 then
            wait()
        end
    end
    
    bodyMovers = {}
    processedParts = {}
end

-- Fungsi update player list - COMPACT VERSION
local function updatePlayerList(listFrame, listLayout, partsValue, playersValue, bringBtn)
    for _, child in pairs(listFrame:GetChildren()) do
        if child:IsA("TextButton") or child:IsA("TextLabel") then
            child:Destroy()
        end
    end
    
    getNearbyPlayers()
    
    if #nearbyPlayers == 0 then
        local NoPlayerLabel = Instance.new("TextLabel")
        NoPlayerLabel.Size = UDim2.new(1, -6, 0, 36)
        NoPlayerLabel.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
        NoPlayerLabel.BorderSizePixel = 0
        NoPlayerLabel.Text = "âŒ No players nearby"
        NoPlayerLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        NoPlayerLabel.TextSize = 10
        NoPlayerLabel.Font = Enum.Font.Gotham
        NoPlayerLabel.Parent = listFrame
        
        local NoPlayerCorner = Instance.new("UICorner")
        NoPlayerCorner.CornerRadius = UDim.new(0, 5)
        NoPlayerCorner.Parent = NoPlayerLabel
        
        local NoPlayerBorder = Instance.new("UIStroke")
        NoPlayerBorder.Color = Color3.fromRGB(0, 255, 0)
        NoPlayerBorder.Thickness = 1
        NoPlayerBorder.Parent = NoPlayerLabel
    else
        for i, data in ipairs(nearbyPlayers) do
            local PlayerBtn = Instance.new("TextButton")
            PlayerBtn.Name = "PlayerBtn_" .. data.player.Name
            PlayerBtn.Size = UDim2.new(1, -6, 0, 32)
            PlayerBtn.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
            PlayerBtn.BorderSizePixel = 0
            PlayerBtn.Text = ""
            PlayerBtn.AutoButtonColor = false
            
            local BtnCorner = Instance.new("UICorner")
            BtnCorner.CornerRadius = UDim.new(0, 5)
            BtnCorner.Parent = PlayerBtn
            
            local BtnBorder = Instance.new("UIStroke")
            BtnBorder.Name = "Border"
            BtnBorder.Color = Color3.fromRGB(0, 200, 0)
            BtnBorder.Thickness = 1
            BtnBorder.Transparency = 0.5
            BtnBorder.Parent = PlayerBtn
            
            -- Avatar Circle
            local AvatarCircle = Instance.new("Frame")
            AvatarCircle.Size = UDim2.new(0, 24, 0, 24)
            AvatarCircle.Position = UDim2.new(0, 5, 0.5, -12)
            AvatarCircle.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
            AvatarCircle.BorderSizePixel = 0
            AvatarCircle.Parent = PlayerBtn
            
            local AvatarCorner = Instance.new("UICorner")
            AvatarCorner.CornerRadius = UDim.new(1, 0)
            AvatarCorner.Parent = AvatarCircle
            
            local AvatarBorder = Instance.new("UIStroke")
            AvatarBorder.Color = Color3.fromRGB(0, 255, 0)
            AvatarBorder.Thickness = 1.5
            AvatarBorder.Parent = AvatarCircle
            
            local AvatarIcon = Instance.new("TextLabel")
            AvatarIcon.Size = UDim2.new(1, 0, 1, 0)
            AvatarIcon.BackgroundTransparency = 1
            AvatarIcon.Text = "ðŸ‘¤"
            AvatarIcon.TextColor3 = Color3.fromRGB(255, 255, 255)
            AvatarIcon.TextSize = 13
            AvatarIcon.Font = Enum.Font.GothamBold
            AvatarIcon.Parent = AvatarCircle
            
            -- Player Name
            local PlayerName = Instance.new("TextLabel")
            PlayerName.Size = UDim2.new(1, -80, 0, 13)
            PlayerName.Position = UDim2.new(0, 33, 0, 5)
            PlayerName.BackgroundTransparency = 1
            PlayerName.Text = data.player.Name
            PlayerName.TextColor3 = Color3.fromRGB(255, 255, 255)
            PlayerName.TextSize = 10
            PlayerName.Font = Enum.Font.GothamBold
            PlayerName.TextXAlignment = Enum.TextXAlignment.Left
            PlayerName.TextTruncate = Enum.TextTruncate.AtEnd
            PlayerName.Parent = PlayerBtn
            
            -- Distance Badge
            local DistanceBadge = Instance.new("Frame")
            DistanceBadge.Size = UDim2.new(0, 55, 0, 14)
            DistanceBadge.Position = UDim2.new(0, 33, 0, 18)
            DistanceBadge.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
            DistanceBadge.BorderSizePixel = 0
            DistanceBadge.Parent = PlayerBtn
            
            local BadgeCorner = Instance.new("UICorner")
            BadgeCorner.CornerRadius = UDim.new(0, 4)
            BadgeCorner.Parent = DistanceBadge
            
            local DistanceText = Instance.new("TextLabel")
            DistanceText.Size = UDim2.new(1, 0, 1, 0)
            DistanceText.BackgroundTransparency = 1
            DistanceText.Text = data.distance .. " m"
            DistanceText.TextColor3 = Color3.fromRGB(255, 255, 255)
            DistanceText.TextSize = 8
            DistanceText.Font = Enum.Font.GothamBold
            DistanceText.Parent = DistanceBadge
            
            -- Selection Indicator
            local SelectIndicator = Instance.new("Frame")
            SelectIndicator.Name = "SelectIndicator"
            SelectIndicator.Size = UDim2.new(0, 3, 0, 22)
            SelectIndicator.Position = UDim2.new(1, -5, 0.5, -11)
            SelectIndicator.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
            SelectIndicator.BorderSizePixel = 0
            SelectIndicator.Visible = false
            SelectIndicator.Parent = PlayerBtn
            
            local IndicatorCorner = Instance.new("UICorner")
            IndicatorCorner.CornerRadius = UDim.new(1, 0)
            IndicatorCorner.Parent = SelectIndicator
            
            -- Hover Effects
            PlayerBtn.MouseEnter:Connect(function()
                BtnBorder.Transparency = 0
                game:GetService("TweenService"):Create(PlayerBtn, TweenInfo.new(0.2), {
                    BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                }):Play()
            end)
            
            PlayerBtn.MouseLeave:Connect(function()
                if targetPlayer ~= data.player then
                    BtnBorder.Transparency = 0.5
                    game:GetService("TweenService"):Create(PlayerBtn, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(15, 15, 15)
                    }):Play()
                end
            end)
            
            -- Click event
            PlayerBtn.MouseButton1Click:Connect(function()
                targetPlayer = data.player
                
                for _, btn in pairs(listFrame:GetChildren()) do
                    if btn:IsA("TextButton") then
                        local indicator = btn:FindFirstChild("SelectIndicator")
                        if indicator then
                            indicator.Visible = false
                        end
                        local border = btn:FindFirstChild("Border")
                        if border then
                            border.Transparency = 0.5
                        end
                        btn.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
                    end
                end
                
                SelectIndicator.Visible = true
                PlayerBtn.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                BtnBorder.Transparency = 0
                
                notify("âœ… Target Selected", data.player.Name .. " (" .. data.distance .. " studs)", 2)
            end)
            
            PlayerBtn.Parent = listFrame
        end
    end
    
    listFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y + 6)
    
    local partCount = scanUnanchoredParts()
    partsValue.Text = tostring(partCount)
    playersValue.Text = tostring(#nearbyPlayers)
end

-- Main Program
local ScreenGui, MainFrame, BringBtn, RefreshBtn, MinimizeBtn, CloseBtn, PlayerListFrame, PartsValue, PlayersValue, ListLayout, BringText, ContentFrame, NeonBorder, RadiusInput, PowerInput = createGUI()

-- Input handling untuk Radius
RadiusInput.FocusLost:Connect(function(enterPressed)
    local input = RadiusInput.Text:gsub("[^%d]", "") -- Hanya ambil angka
    local value = tonumber(input)
    
    if value and value >= 50 and value <= 1000 then
        Settings.Radius = value
        RadiusInput.Text = value .. " studs"
        updatePlayerList(PlayerListFrame, ListLayout, PartsValue, PlayersValue, BringBtn)
        notify("âœ… Radius Updated", "Set to " .. value .. " studs", 2)
    else
        RadiusInput.Text = Settings.Radius .. " studs"
        notify("âš ï¸ Invalid", "Radius must be 50-1000", 2)
    end
end)

-- Input handling untuk Power
PowerInput.FocusLost:Connect(function(enterPressed)
    local input = PowerInput.Text:gsub("[^%d]", "")
    local value = tonumber(input)
    
    if value and value >= 5000 and value <= 200000 then
        Settings.Power = value
        Settings.Damping = math.floor(value / 10)
        Settings.VelocityBoost = math.floor(50 + ((value - 5000) / 195000) * 150)
        PowerInput.Text = tostring(value)
        notify("âœ… Power Updated", "Set to " .. value, 2)
    else
        PowerInput.Text = tostring(Settings.Power)
        notify("âš ï¸ Invalid", "Power must be 5000-200000", 2)
    end
end)

updatePlayerList(PlayerListFrame, ListLayout, PartsValue, PlayersValue, BringBtn)

notify("âœ… Loaded", "Compact GUI Ready!", 3)
notify("ðŸŽ¯ Select", "Choose target from list", 2)

-- Event Handlers
BringBtn.MouseButton1Click:Connect(function()
    if not bringing then
        if not targetPlayer then
            notify("âš ï¸ Warning", "Select target first!", 3)
            return
        end
        
        if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            notify("âŒ Error", "Invalid target!", 3)
            return
        end
        
        bringing = true
        BringText.Text = "PULLING..."
        
        local TweenService = game:GetService("TweenService")
        TweenService:Create(BringBtn, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        }):Play()
        
        notify("âœ… Started", "Pulling parts to " .. targetPlayer.Name, 2)
        
        currentPartIndex = 1
        processedParts = {}
        
        bringConnection = RunService.Heartbeat:Connect(function()
            if bringing then
                bringPartsPhysical()
            end
        end)
    else
        bringing = false
        BringText.Text = "BRING"
        
        local TweenService = game:GetService("TweenService")
        TweenService:Create(BringBtn, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        }):Play()
        
        if bringConnection then
            bringConnection:Disconnect()
        end
        
        cleanupBodyMovers()
        
        notify("â¹ï¸ Stopped", "Pulling stopped", 2)
    end
end)

RefreshBtn.MouseButton1Click:Connect(function()
    updatePlayerList(PlayerListFrame, ListLayout, PartsValue, PlayersValue, BringBtn)
    notify("ðŸ”„ Refreshed", "Data updated!", 2)
end)

-- Minimize functionality
local isMinimized = false
MinimizeBtn.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    
    local TweenService = game:GetService("TweenService")
    
    if isMinimized then
        ContentFrame.Visible = false
        
        TweenService:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 270, 0, 42)
        }):Play()
        MinimizeBtn.Text = "â˜"
    else
        ContentFrame.Visible = true
        
        TweenService:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 270, 0, 385)
        }):Play()
        MinimizeBtn.Text = "â”€"
    end
end)

-- Close functionality
CloseBtn.MouseButton1Click:Connect(function()
    if bringing and bringConnection then
        bringConnection:Disconnect()
    end
    
    cleanupBodyMovers()
    ScreenGui:Destroy()
    notify("ðŸ‘‹ Closed", "GUI closed", 2)
end)

-- Auto refresh player list
spawn(function()
    while ScreenGui.Parent do
        wait(3)
        if not bringing then
            updatePlayerList(PlayerListFrame, ListLayout, PartsValue, PlayersValue, BringBtn)
        end
    end
end)

-- Animasi loading
spawn(function()
    local TweenService = game:GetService("TweenService")
    MainFrame.Size = UDim2.new(0, 270, 0, 0)
    TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 270, 0, 385)
    }):Play()
end)

