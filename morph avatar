    -- Button functions
    resetBtn.MouseButton1Click:Connect(function()
        spawn(function()
            resetBtn.Text = "..."
            resetBtn.BackgroundColor3 = Color3.fromRGB(255, 193, 7)
            local success = SimpleReset()
